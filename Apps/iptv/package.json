  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@eva-design/eva": "*",
    "@testing-library/jest-native": "*",
    "@testing-library/react-native": "*",
    "@ui-kitten/components": "*",
    "metro-config": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-safe-area-context": "*",
    "expo": "*",
    "react-native-svg": "*",
    "react-native-web": "*"
  },
  "scripts": {
